{
  "name": "fxchange-backend",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run clean && npx prisma generate && npm run compile",
    "clean": "rm -rf ./dist/",
    "compile": "tsc --build tsconfig.prod.json",
    "lint": "npx eslint --ext .ts src/",
    "lint:tests": "npx eslint --ext .ts spec/",
    "start": "node -r module-alias/register ./dist --env=production",
    "dev": "set NODE_ENV=development && npx prisma generate && nodemon",
    "test": "nodemon --config ./spec/nodemon.json",
    "test:no-reloading": "npx ts-node --files -r tsconfig-paths/register ./spec"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@apollo/server": "^4.7.5",
    "@googlemaps/google-maps-services-js": "^3.3.32",
    "@graphql-tools/graphql-file-loader": "^8.0.0",
    "@graphql-tools/load": "^8.0.0",
    "@graphql-yoga/plugin-defer-stream": "^1.9.1",
    "@graphql-yoga/plugin-graphql-sse": "^2.0.3",
    "@prisma/client": "^4.16.2",
    "@types/firebase": "^3.2.1",
    "@types/socket.io": "^3.0.2",
    "@whatwg-node/fetch": "^0.9.7",
    "@whatwg-node/server-plugin-cookies": "^0.0.7",
    "agenda": "^5.0.0",
    "bcrypt": "^5.1.0",
    "bson": "^5.3.0",
    "bull": "^4.10.4",
    "child_process": "^1.0.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "events": "^3.3.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "firebase": "^9.23.0",
    "firebase-admin": "^11.9.0",
    "google-auth-library": "^8.9.0",
    "graphql": "^16.7.1",
    "graphql-middleware": "^6.1.34",
    "graphql-shield": "^7.6.5",
    "graphql-sse": "^2.2.0",
    "graphql-upload-ts": "^2.0.6",
    "graphql-yoga": "^3.9.1",
    "helmet": "^7.0.0",
    "inserturlparams": "^1.0.1",
    "jet-logger": "^1.3.1",
    "jet-validator": "^1.1.1",
    "jsdom": "^22.1.0",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "nexus-plugin-prisma": "^0.35.0",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.3",
    "rollbar": "^2.26.2",
    "sharp": "^0.32.1",
    "short-unique-id": "^4.4.4",
    "slugify": "^1.6.6",
    "socket.io": "^4.7.1",
    "sse-pubsub": "^1.4.2",
    "ts-command-line-args": "^2.5.1",
    "unique-names-generator": "^4.7.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jasmine": "^4.3.5",
    "@types/jsonfile": "^6.1.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-plugin-node": "^11.1.0",
    "find": "^0.3.0",
    "fs-extra": "^11.1.1",
    "jasmine": "^5.0.2",
    "nodemon": "^2.0.22",
    "prisma": "^4.16.2",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
